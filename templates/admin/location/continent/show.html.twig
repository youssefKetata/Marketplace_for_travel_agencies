{% extends 'base.html.twig' %}

{% block title %}Continent{% endblock %}

{% block body %}
    <h1>Continent</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Code</th>
                <td>{{ continent.code }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ continent.name }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ continent.active ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_location_continent_index') }}">back to list</a>

    <a href="{{ path('app_admin_location_continent_edit', {'code': continent.code}) }}">edit</a>

    {{ include('admin/location/continent/_delete_form.html.twig') }}
{% endblock %}
