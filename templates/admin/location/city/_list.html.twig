{% for city in cities %}
    <tr>
        <td>{{ city.id }}</td>
        <td>{{ city.name }}</td>
        <td>{{ city.latitude }}</td>
        <td>{{ city.longitude }}</td>
        <td>{{ city.active ? 'Yes' : 'No' }}</td>
        <td>
            <button

                    class="btn"
                    data-action="modal-form#openModal"
                    data-form-url-value="{{ path('app_admin_location_city_edit', {'id': city.id}) }}"
                    data-url-value="{{ path('app_admin_location_city_index', { ajax: 1 } ) }}"
                    data-node-dest-id-value="myOneCustomContent"
                    data-modal-title-value="Edit"
                    data-refresh-key-value="refresh-html"
            >
                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor"
                     class="bi bi-pencil-square text-dark" viewBox="0 0 16 16">
                    <path d="M15.502 1.94a.5.5 0 0 1 0 .706L14.459 3.69l-2-2L13.502.646a.5.5 0 0 1 .707 0l1.293 1.293zm-1.75 2.456-2-2L4.939 9.21a.5.5 0 0 0-.121.196l-.805 2.414a.25.25 0 0 0 .316.316l2.414-.805a.5.5 0 0 0 .196-.12l6.813-6.814z"></path>
                    <path fill-rule="evenodd"
                          d="M1 13.5A1.5 1.5 0 0 0 2.5 15h11a1.5 1.5 0 0 0 1.5-1.5v-6a.5.5 0 0 0-1 0v6a.5.5 0 0 1-.5.5h-11a.5.5 0 0 1-.5-.5v-11a.5.5 0 0 1 .5-.5H9a.5.5 0 0 0 0-1H2.5A1.5 1.5 0 0 0 1 2.5v11z"></path>
                </svg>
            </button>
            <button

                    class="btn"
                    data-action="modal-form#openModal"
                    data-form-url-value="{{ path('app_admin_location_city_delete', {'id': city.id}) }}"
                    data-url-value="{{ path('app_admin_location_city_index', { ajax: 1 } ) }}"
                    data-node-dest-id-value="myOneCustomContent"
                    data-modal-title-value="Delete"
                    data-refresh-key-value="refresh-html"
            >
                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor"
                     class="bi bi-trash-fill text-dark" viewBox="0 0 16 16">
                    <path d="M2.5 1a1 1 0 0 0-1 1v1a1 1 0 0 0 1 1H3v9a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2V4h.5a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H10a1 1 0 0 0-1-1H7a1 1 0 0 0-1 1H2.5zm3 4a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7a.5.5 0 0 1 .5-.5zM8 5a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7A.5.5 0 0 1 8 5zm3 .5v7a.5.5 0 0 1-1 0v-7a.5.5 0 0 1 1 0z"/>
                </svg>
            </button>
            {# <a href="{{ path('app_admin_location_city_show', {'id': city.id}) }}">show</a> #}
            {# <a href="{{ path('app_admin_location_city_edit', {'id': city.id}) }}">edit</a> #}

            {#                <span {{ stimulus_controller('modal-form', { #}
            {#                    formUrl: path('app_admin_location_city_edit', {'id': city.id}) }) }} #}
            {#                        data-action="hidden.bs.modal->modal-form#modalHidden" #}
            {#                > #}
            {#                    {% include 'buttons_grid.html.twig' with {'show': 'false', 'edit': 'true', 'delete': 'false', 'add': 'false',} %} #}

            {#                </span> #}
        </td>
    </tr>
{% else %}
    <tr>
        <td colspan="6">no records found</td>
    </tr>
{% endfor %}
