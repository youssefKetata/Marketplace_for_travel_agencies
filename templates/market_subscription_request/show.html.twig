{% extends 'base.html.twig' %}

{% block title %}MarketSubscriptionRequest{% endblock %}

{% block body %}
    <h4>Market subscription request</h4>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ market_subscription_request.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ market_subscription_request.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ market_subscription_request.email }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ market_subscription_request.website }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ market_subscription_request.address }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ market_subscription_request.city }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ market_subscription_request.status }}</td>
            </tr>
        </tbody>
    </table>
    {{ include('market_subscription_request/_delete_form.html.twig') }}
{% endblock %}
